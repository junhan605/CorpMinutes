

<%= simple_form_for @minute, url: company_minutes_path(@company) do |f| %>
  <fieldset class="inputs" id="Adopt_Bylaws">
    <ol>

      <li class="string input optional stringish">
        <%= f.input :location %>
      </li>

      <li class="string input optional stringish">
        <div class="field">
          <%= f.label :present %>
        </div>
      </li>

      <li class="boolean input optional">
        <div class="field">
          <label>
            <%= check_box_tag "michael"%>
            MICHEAL (PRESIDENT)
          </label>
        </div>
      </li>

      <li class="boolean input optional">
        <div class="field">
          <label>
            <%= check_box_tag "sonny"%>
            SONNY (TREASURER)
          </label>
        </div>
      </li>

      <li class="boolean input optional">
        <div class="field">
          <label>
            <%= check_box_tag "kim"%>
            KIM (DIRECTOR)
          </label>
        </div>
      </li>

      <li class="string input optional stringish">
        <%= f.input :other_party %>
      </li>

      <li class="string input optional stringish">
        <%= f.input :escriibe_property %>
      </li>
    </ol>
  </fieldset>

  <div class="modal-footer">
    <fieldset class="actions">
      <ol>
        <li class="action input_action">
          <%= f.submit class: "btn btn-primary" %>
        </li>
        <li class="cancel">
          <%= link_to "Cancel", "#", class: "btn", data: {dismiss: "modal"} %>
        </li>
      </ol>
    </fieldset>
  </div>

<% end %>
